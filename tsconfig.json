{
  "extends": "@backstage/cli/config/tsconfig.json",
  "include": [
    "packages/*/src",
    "plugins/*/src",
    "plugins/*/dev",
    "plugins/*/migrations"
  ],
  "exclude": ["node_modules", "./node_modules", "./node_modules/*"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": "dist-types",
    "rootDir": ".",
    "useUnknownInCatchVariables": false
  }
}
